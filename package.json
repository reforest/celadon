  },
  "homepage": "https://github.com/reforest/celadon#readme",
  "dependencies": {
    "lambocoin": "0.0.13",
    "reforest-fungus": "^1.0.2",
    "rm": "^0.1.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
